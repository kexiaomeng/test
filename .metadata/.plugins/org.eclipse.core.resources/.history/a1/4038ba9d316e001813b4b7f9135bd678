		marshallingDecoder  = new MarshallingDecoder();
	}

	private MarshallingDecoder marshallingDecoder;
	@Override
	protected Object decode(ChannelHandlerContext ctx, ByteBuf in)
    throws Exception{
		
	}

	

}
